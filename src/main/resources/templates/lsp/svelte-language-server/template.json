{
  "id": "svelte-language-server",
  "name": "Svelte Language Server",
  "programArgs": {
    "windows": "svelteserver.cmd --stdio",
    "default": "svelteserver --stdio"
  },
  "expandConfiguration": true,
  "fileTypeMappings": [
    {
      "fileType": {
        "patterns": [
          "*.svelte"
        ]
      },
      "languageId": "svelte"
    }
  ]
}